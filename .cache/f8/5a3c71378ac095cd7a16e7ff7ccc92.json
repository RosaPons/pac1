    background-size: cover;\r\n    padding-left: 3vw;\r\n}\r\n\r\n/* capçaleres que apareixen en ind-presentacio-banner */\r\n\r\n\r\n#ind-presentacio-banner h1, \r\n#ind-presentacio-banner h2,\r\n#ind-presentacio-banner h3,\r\n#ind-presentacio-banner h3 a {\r\n    color:  #fdf39b; \r\n    font-weight: 800;\r\n}\r\n\r\n#ind-presentacio-banner h3 a:hover{\r\n    color:  #ffe81b;\r\n}\r\n/* Mides de fonts de capçalera */\r\n#ind-presentacio-banner h1 {\r\n    font-size: 1.3rem;\r\n    line-height: 1.1em; \r\n    margin-top: 0.5em;\r\n}\r\n#ind-presentacio-banner h2 {\r\n    font-size: 1rem;\r\n    line-height: 0.5em;\r\n    margin-top: 0.5em;\r\n    margin-bottom: 1.5em;\r\n}\r\n#ind-presentacio-banner h3 {\r\n    font-size: 1rem;\r\n    line-height: 0.5em;\r\n    margin-top: 1.5em;\r\n    margin-bottom: 1.5em;\r\n}\r\n@media (min-width: 568px) {\r\n    #ind-presentacio-banner h1 {\r\n       font-size: 2rem;\r\n   }\r\n}\r\n@media (min-width: 768px) {\r\n    #ind-presentacio-banner h1 {\r\n       font-size: 2.7rem;\r\n       line-height: 1.1em;         \r\n       padding-left: 11vw;\r\n   }\r\n   #ind-presentacio-banner h2 {\r\n       font-size: 1.5rem;\r\n       line-height: 1.1em;\r\n       padding-left: 11vw;\r\n   }\r\n   #ind-presentacio-banner h3 {\r\n       font-size: 1.3rem;\r\n       line-height: 1.1em;\r\n       padding-left: 11vw;\r\n   }\r\n}\r\n\r\n/* -- Main: 2ona secció passajades culturals:  */\r\n/* contenidor de secció */\r\n#ind-section-passajades-culturals {\r\n    padding-left: 3vw;\r\n    padding-right: 3vw;\r\n    padding-top: 1.25rem;\r\n}\r\n@media (min-width: 768px) { \r\n    #ind-section-passajades-culturals{\r\n        margin-left: 11vw;\r\n        margin-right: 11vw;\r\n        padding-top: 3rem;\r\n    }\r\n}\r\n/* Títol secció */\r\n#ind-section-passajades-culturals h2{\r\n    color:  black;\r\n    margin-bottom: 1.25rem;\r\n    font-size: 1.5rem;\r\n}\r\n@media (min-width: 768px) { \r\n    #ind-section-passajades-culturals h2{\r\n        font-size: 2rem;\r\n    }\r\n}\r\n/* Contenidor principal de les fotografies */\r\n#ind-passajades-culturals{\r\n    width: 100%;\r\n}\r\n/* Contenidor principal de les fotos per a dispositius a partir de 768px */\r\n@media (min-width: 768px) { \r\n    #ind-passajades-culturals{\r\n       width: 100%;\r\n       display: flex;    \r\n       flex-direction: row;\r\n       justify-content: center;\r\n       margin-bottom: 2.5rem;\r\n    }   \r\n}\r\n\r\n/* -- Main: 3era secció barcelona literària */\r\n\r\n/* aplicar estil a nivell de seccion per dispositius dins a 768px */\r\n#ind-section-barcelona-literaria{\r\n    padding-top: 1.25rem;\r\n    padding-left: 3vw;\r\n    padding-right: 3vw;\r\n    margin-bottom: 1rem;\r\n}\r\n@media (min-width: 768px) { \r\n    #ind-section-barcelona-literaria{\r\n        margin-left: 11vw;\r\n        margin-right: 11vw;\r\n        margin-bottom: 1.25rem;\r\n    }\r\n}\r\n\r\n/* títol secció: barcelona literaria */\r\n#ind-section-barcelona-literaria h2 {\r\n    color:  black;\r\n    margin-bottom: 0.5rem;\r\n    font-size: 1.5rem;\r\n}\r\n@media (min-width: 768px) { \r\n    #ind-section-barcelona-literaria h2 {\r\n        margin-bottom: 1rem;\r\n        font-size: 2rem;\r\n    }\r\n}\r\n\r\n/* text explicatiu de barcelona literaria */\r\n#ind-section-barcelona-literaria h3 {\r\n    color:  black;\r\n    font-weight: 400;\r\n    padding-top: 0.6rem;\r\n    padding-bottom:  0.6rem;\r\n    font-size: 1rem;\r\n}\r\n @media (min-width: 768px) { \r\n    #ind-section-barcelona-literaria h3 {\r\n        font-size: 1.5rem;\r\n        margin-right: 15%;\r\n    }\r\n}\r\n\r\n/* Contenidor principal de les fotos */\r\n#ind-bcn-literaria{\r\n    width: 100%;\r\n}\r\n/* Contenidor principal de les fotos per a dispositius a partir de 768px */\r\n@media (min-width: 768px) { \r\n    #ind-bcn-literaria{\r\n        width: 100%;\r\n        display: flex;    \r\n        flex-direction: row;\r\n        justify-content: space-between;\r\n    }\r\n}\r\n\r\n/*********\r\n   Fotos\r\n**********/\r\n\r\n/* contenidor per a cada foto */\r\n.ind-foto {\r\n    width: 100%;\r\n    padding: 0.31rem;\r\n}\r\n/* imatges escalables */\r\n.ind-foto img {\r\n    width: 100%;\r\n    height: auto;\r\n    border-radius: 25px;\r\n    -webkit-border-radius: 25px;\r\n    -moz-border-radius: 25px;\r\n    -ms-border-radius: 25px;\r\n    -o-border-radius: 25px;\r\n}\r\n\r\n/* text que apareix al peu de les fotos */\r\n.figcaption-text {\r\n    text-align: left;\r\n    margin-bottom: 1.25rem;\r\n    font-size: 1rem;\r\n    text-align: center;\r\n    font-weight: bold;\r\n \r\n}\r\n/* text que apareix al peu de les fotos  per a dispositius a partir de 768px: centrat */\r\n@media (min-width: 570px) { \r\n    .figcaption-text {\r\n        font-size: 1rem;\r\n    }\r\n}\r\n@media (min-width: 768px) { \r\n    .figcaption-text {\r\n        font-size: 1rem;\r\n    }\r\n}\r\n@media (min-width: 992px) { \r\n    .figcaption-text {\r\n        font-size: 1.3rem;\r\n    }\r\n}\r\n\r\n/* --- fi --- */"},"lineCount":null}},"error":null,"hash":"97ae8e0ef23ee3e1f3e996d4bd80e83a","cacheData":{"env":{}}}